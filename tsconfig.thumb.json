{
  "extends": "ts-node/node16/tsconfig.json",
  "ts-node": {
    "esm": true
  },
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
  "include": ["config/*.ts", "scripts/*.ts"]
}
