{
  "extends": "maptalks",
  "globals": {
    "$": true,
    "d3": true,
    "topojson": true,
    "subdomains": true,
    "echarts": true,
    "Highcharts": true
  },
  "rules": {
    "strict": 0,
    "comma-spacing": 0,
    "space-before-function-paren": "off",
    "indent": ["error", 2],
    "no-var": "off",
    "no-loop-func": "off",
    "no-undef": "off",
    "no-useless-escape": "off",
    "quotes": "off",
    "no-unused-vars": "off",
    "global-require": "off",
    "no-use-before-define": "off",
    "no-loss-of-precision": "off"
  },
  "env": {
    "es6": true
  }
}
